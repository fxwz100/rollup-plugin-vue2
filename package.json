{
  "name": "rollup-plugin-vue",
  "version": "5.1.9",
  "description": "Roll .vue files",
  "author": "Rahul Kadyan <hi@znck.me>",
  "bugs": {
    "url": "https://github.com/vuejs/rollup-plugin-vue/issues"
  },
  "homepage": "https://github.com/vuejs/rollup-plugin-vue#readme",
  "keywords": [
    "rollup-plugin",
    "vue"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/rollup-plugin-vue.mjs",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/rollup-plugin-vue.git"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "yarn test && yarn :build:docs && git add docs/"
    }
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist/ && yarn build",
    "dev": "tsc -w",
    "build": "tsc",
    "docs": "yarn :docs",
    "pre:build": "yarn lint",
    "pre:build:docs": "cp CHANGELOG.md docs/changelog.md",
    ":build:docs": "vuepress build docs/",
    "post:build:docs": "rm docs/changelog.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "pre:docs": "cp CHANGELOG.md docs/changelog.md",
    ":docs": "vuepress dev docs/",
    "post:docs": "rm docs/CHANGELOG.md",
    "lint": "prettier --no-semi --single-quote --write **/*.js **/*.ts **/*.vue !test/target/** !dist/**",
    "release": "standard-version -a",
    "test": "jest"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@vue/component-compiler": "^4.2.3",
    "@vue/component-compiler-utils": "^3.1.2",
    "debug": "^4.1.1",
    "hash-sum": "^1.0.2",
    "magic-string": "^0.25.7",
    "querystring": "^0.2.0",
    "rollup-pluginutils": "^2.8.2",
    "source-map": "0.7.3",
    "vue-runtime-helpers": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/runtime": "^7.0.0-beta.46",
    "@types/debug": "^0.0.31",
    "@types/hash-sum": "^1.0.0",
    "@types/jest": "^23.3.12",
    "@types/node": "^16.10.3",
    "@types/puppeteer": "^1.3.1",
    "@znck/promised": "^1.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "conventional-changelog": "^3.0.5",
    "jest": "^23.6.0",
    "postcss": "^7.0.11",
    "postcss-assets": "^5.0.0",
    "prettier": "^2.5.1",
    "pug": "^2.0.3",
    "puppeteer": "^1.4.0",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-md": "^0.0.7",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-url": "^2.1.0",
    "sass": "^1.18.0",
    "ts-jest": "^23.10.5",
    "typescript": "^4.4.4",
    "vue": "^2.6.14",
    "vue-class-component": "^6.2.0",
    "vue-template-compiler": "^2.6.14",
    "vuepress": "^0.14.8"
  },
  "peerDependencies": {
    "vue-template-compiler": "*"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  }
}
